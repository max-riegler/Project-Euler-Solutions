   "source": [
    "def fib(n):\n",
    "    a, b = 1, 1\n",
    "    counter = 0\n",
    "    while counter < n:\n",
    "        a, b = b, a + b\n",
    "        counter += 1\n",
    "    return b\n",
    "\n",
    "n = 0\n",
    "a = 0\n",
    "\n",
    "while fib(a) <= 4000000:\n",
    "    if fib(a) % 2 == 0:\n",
    "        n = n + fib(a)\n",
    "        a += 1\n",
    "    else:\n",
    "        a += 1\n",
    "\n",
    "print(n)"
   ]
  }
