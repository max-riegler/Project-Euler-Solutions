   "source": [
    "def check(n,i,j):\n",
    "    counter = 0\n",
    "    for k in range(i,j):\n",
    "        if n % k == 0:\n",
    "            counter += 1\n",
    "    if counter == j-1:\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "\n",
    "num = 20    \n",
    "\n",
    "while check(num,1,20) == False:\n",
    "    num += 20\n",
    "\n",
    "print(num)    "
   ]
  }
