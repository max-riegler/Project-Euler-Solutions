   "source": [
    "def sq_sum(a,b):\n",
    "    Sum = 0\n",
    "    for i in range(a,b + 1):\n",
    "        Sum = Sum + i ** 2\n",
    "    return Sum\n",
    "\n",
    "def sum_sq(a,b):\n",
    "    Sum = 0\n",
    "    for i in range(a,b + 1):\n",
    "        Sum = Sum + i\n",
    "    return Sum ** 2\n",
    "\n",
    "print(sq_sum(1,100))\n",
    "print(sum_sq(1,100))\n",
    "print(sum_sq(1,100) - sq_sum(1,100))"
   ]
  }
